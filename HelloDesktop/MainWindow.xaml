<Window x:Class="HelloDesktop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloDesktop"
        mc:Ignorable="d"
        Title="Hello Desktop" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="197*"/>
            <ColumnDefinition Width="595*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Content="Hello World!!" HorizontalAlignment="Left" Margin="10,166,0,0" VerticalAlignment="Top" FontSize="64" Grid.ColumnSpan="2"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="151" Margin="10,10,0,0" VerticalAlignment="Top" Width="494" Source="QUA-OpU-RMS-Welding.png" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
